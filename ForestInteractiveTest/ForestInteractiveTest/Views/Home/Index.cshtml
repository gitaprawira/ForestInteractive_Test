@model ForestInteractiveTest.Models.Transaction

<!--begin::Portlet-->
<div class="m-portlet m-portlet--tab">
    <div class="m-portlet__head">
        <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
                <span class="m-portlet__head-icon m--hide">
                    <i class="la la-gear"></i>
                </span>
                <h3 class="m-portlet__head-text">
                    Import Contact Group
                </h3>
            </div>
        </div>
    </div>
    <!--begin::Form-->
    <form class="m-form m-form--fit m-form--label-align-right">
        <div class="m-portlet__body">
            <form id="regexpForm" class="form-horizontal">
                <div class="form-group m-form__group">
                    <label>
                        Select file to upload (.xls)
                    </label>
                    <div></div>
                    <div class="file-loading">
                        <input id="FileUpload" name="FileUpload" type="file" multiple>
                    </div>
                    <br />
                </div>
                <div class="form-group m-form__group">
                    <label>
                        Message
                    </label>
                    <textarea  id="message" class="form-control m-input" rows="3"></textarea>
                    <div id="regex" class="text-danger hidden" style="display:none;">
                        Please input 20 to 160 characters and Cannot put special character
                    </div>
                </div>
                <div class="m-form__actions">
                    <button type="button" id="modal" class="btn btn-primary" data-target="#ajax" data-toggle="modal" disabled="disabled">
                        Submit
                    </button>
                </div>
            </form>
        </div>
        <div class="m-portlet__foot m-portlet__foot--fit">
            <div class="m-portlet m-portlet--mobile">
                <div class="m-portlet__head">
                    <div class="m-portlet__head-caption">
                        <div class="m-portlet__head-title">
                            <h3 class="m-portlet__head-text">
                                Contact List
                            </h3>
                        </div>
                    </div>
                </div>
                <div class="m-portlet__body">
                    @{Html.RenderAction("ContactList", "Home");}
                </div>
            </div>
        </div>
    </form>
    <!--end::Form-->
</div>
<!--end::Portlet-->

<!--begin::Modal-->
<div aria-hidden="true" role="dialog" id="ajax" tabindex="-1" class="modal fade bs-modal-lg">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" style="width:400px">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                <h4 class="modal-title" style="font-weight: bold;">Warning!</h4>
            </div>
            <div class="modal-body">
                Are you sure you want to save ?
            </div>
            <div class="modal-footer">
                <a id="save" class="btn btn-info btn-ok">Save</a>
                <button type="button" class="btn default btn-circle modal-close" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<!--end::Modal-->

<script type="text/javascript" src="~/Scripts/jquery-3.3.1.min.js"></script>
<script type="text/javascript" src="~/Scripts/fileinput.min.js"></script>
<script type="text/javascript" src="~/Scripts/Site.js"></script>